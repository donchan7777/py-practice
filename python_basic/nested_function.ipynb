   "id": "f49bcc69-f8a3-436e-bf36-9b9179f4aed1",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "this is inner function\n",
      "inner\n",
      "inner\n",
      "global\n"
     ]
    }
   ],
   "source": [
    "msg = \"global\"\n",
    "\n",
    "\n",
    "def outer():\n",
    "    msg = \"outer\"\n",
    "    \n",
    "    \n",
    "    def inner():\n",
    "        nonlocal msg\n",
    "        msg = \"inner\"\n",
    "        print(\"this is inner function\")\n",
    "\n",
    "        print(msg)\n",
    "    inner()\n",
    "    print(msg)\n",
    "\n",
    "    \n",
    "outer()\n",
    "print(msg)\n",
    "\n"
   ]
  },
